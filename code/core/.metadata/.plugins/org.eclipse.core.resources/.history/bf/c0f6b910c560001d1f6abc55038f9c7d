
public class Demo2DArray {
	
	public static void main(String[] args) {
		//how to create array in java
		int x[][]=new int[4][5];
	
		System.out.println(x[0].length);
		
		
		
	//	print2D(x);
		
	}

	static void print2D(int[][] x) {
		for(int temp[]:x) {
			for(int temp2: temp) {
				System.out.print(temp2+" ");
			}
			System.out.println();
		}
	}

}
