<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context-4.3.xsd">

   <context:annotation-config/>
    <bean id="foobean" class="com.demo.Foo" >
        <property name="name" value="foo value"/>
    </bean>

    <bean id="barbean" class="com.demo.Bar" >
        <property name="name" value="bar value"/>
    </bean>
    <bean class="com.demo.BPP1"/>

    <bean class="com.demo.BPP2"/>


    <bean class="com.demo.BeanFPP"/>

</beans>




